{
    "name": "gpii-diff",
    "version": "1.0.0",
    "description": "A library for describing the differences between two JSON structures.",
    "main": "index.js",
    "scripts": {
        "pretest": "node node_modules/rimraf/bin.js reports/* coverage/*",
        "test": "npm run test:node && npm run test:browser",
        "test:node": "node node_modules/nyc/bin/nyc.js node tests/node-tests.js",
        "test:browser": "node node_modules/testem/testem.js --file tests/testem.js ci --skip Safari,PhantomJS --launch Chrome",
        "posttest": "node node_modules/nyc/bin/nyc.js report -r text-summary -r html --report-dir reports --temp-directory coverage"
    },
    "bugs": {
        "url": "https://issues.gpii.net/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GPII/gpii-diff.git"
    },
    "author": "Tony Atkins <tony@raisingthefloor.org>",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/GPII/gpii-diff#readme",
    "dependencies": {
        "gpii-handlebars": "2.1.0",
        "infusion": "3.0.0-dev.20200214T151835Z.ab768289a.FLUID-6145"
    },
    "devDependencies": {
        "cheerio": "1.0.0-rc.3",
        "eslint": "6.8.0",
        "eslint-config-fluid": "1.4.0",
        "gpii-grunt-lint-all": "1.0.7",
        "gpii-launcher": "1.0.0-dev.20170712T140305Z.aa60d4a",
        "gpii-testem": "2.1.11",
        "grunt": "1.0.4",
        "markdown-it": "10.0.0",
        "node-jqunit": "1.1.8",
        "nyc": "13.1.0",
        "rimraf": "3.0.2",
        "svg.js": "2.7.1",
        "svgdom": "0.0.21"
    }
}
